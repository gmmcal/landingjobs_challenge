<%@companies.in_groups_of(6, false) do |companies| %>
  <div class="row">
    <%companies.each do |company| %>
      <div class="col-md-2">
        <%=link_to image_tag(company.logo_url, alt: company.name), company.website_url, target: '_blank', alt: company.name %>
        <%=company.name%>
      </div>
    <%end%>
  </div>
<%end%>
